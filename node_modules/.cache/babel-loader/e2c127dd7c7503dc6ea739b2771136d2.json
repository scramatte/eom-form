leSubmit\" novalidate>\n        <EomForm\n          :schema=\"schema\"\n          v-model=\"model\"\n          ref=\"eomForm\"\n        />\n        <b-btn variant=\"success\" type=\"submit\">submit</b-btn>\n      </form>\n    </b-container>\n  </div>\n</template>\n\n<script>\nimport schema from './data/schema.json'\nimport EomForm from './components/EomForm.vue'\n\nexport default {\n  name: 'App',\n  components: {\n    EomForm,\n  },\n  data () {\n    return {\n      status: {\n        message: '',\n        variant: 'success'\n      },\n      schema,\n      model: {},\n    }\n  },\n  methods: {\n    handleSubmit () {\n      this.status.message = ''\n      let valid = this.$refs.eomForm.validate()\n\n      if (!valid) {\n        console.warn('invalid form')\n        this.status.message = 'invalid_datas'\n        this.status.variant = 'danger'\n        return\n      }\n    }\n  }\n}\n</script>\n"],"sourceRoot":"src"}]}